{
    "name": "syamsoul/laravel-datatable-ssp",
    "description": "DataTable SSP for Laravel. This package allows you to manage your DataTable from server-side in Laravel app.",
    "keywords": [
        "syamsoul",
        "laravel",
        "datatable",
        "php",
        "ssp",
        "souldoit",
        "server-side"
    ],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Syamsoul Azrien Muda",
            "email": "syamsoulazrien88@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "SoulDoit\\DataTable\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoulDoit\\DataTable\\DataTableServiceProvider"
            ]
        }
    }
}
